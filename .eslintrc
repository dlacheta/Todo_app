{
    "extends": ["airbnb", "airbnb-typescript", "prettier"],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
    }
  }